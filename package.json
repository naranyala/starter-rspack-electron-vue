{
  "name": "starter-rspack-electron-vue",
  "version": "0.1.2",
  "description": "A starter template for building Electron applications with Vue.js and Rspack.",
  "scripts": {
    "deps:latest": "bunx npm-check-updates -u -p bun",
    "dev": "tsx scripts/dev.ts dev",
    "dev:legacy": "bunx rspack serve",
    "rspack-dev": "bunx rspack serve",
    "electron-dev": "bunx electron main.cjs --start-dev",
    "start": "bunx electron main.cjs",
    "build": "tsx scripts/dev.ts build",
    "build:legacy": "bunx rspack build",
    "rspack-build": "bunx rspack build",
    "package": "tsx scripts/dev.ts package",
    "package:legacy": "bun run electron-dist",
    "electron-pack": "bunx electron-builder --dir",
    "electron-dist": "bunx electron-builder",
    "electron-postinstall": "bunx electron-builder install-app-deps",
    "dist": "tsx scripts/dev.ts package",
    "setup": "tsx scripts/setup.ts",
    "type-check": "tsc --noEmit --project tsconfig.check.json",
    "lint": "bunx @biomejs/biome check --write .",
    "lint-check": "bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome format --write .",
    "format-check": "bunx @biomejs/biome format .",
    "compile": "tsc --project tsconfig.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "author": "naranyala",
  "build": {
    "appId": "com.some.electron.quickstart",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "dist/icon.ico"
    },
    "win": {
      "target": "msi",
      "icon": "dist/icon.ico"
    },
    "files": [
      "main.cjs",
      "dist/**/*"
    ]
  },
  "keywords": [
    "rspack",
    "vue",
    "electron"
  ],
  "license": "MIT",
  "dependencies": {
    "get-port": "^7.1.0",
    "vue": "^3.5.27",
    "winbox": "^0.2.82"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@rspack/cli": "^1.7.5",
    "@rspack/core": "^1.7.5",
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/compiler-sfc": "^3.5.27",
    "css-loader": "^7.1.3",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "electron-reload": "^1.5.0",
    "html-webpack-plugin": "^5.6.6",
    "jsdom": "^28.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "style-loader": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-loader": "^17.4.2",
    "wait-on": "^9.0.3"
  }
}
